{
    "name": "sf4/api-user",
    "type": "library",
    "description": "User module",
    "keywords": [
        "sf4",
        "api-user"
    ],
    "homepage": "https://github.com/sf4/api-user",
    "license": "MIT",
    "authors": [
        {
            "name": "Siim Liimand",
            "email": "siim.liimand@gmail.com",
            "homepage": "https://github.com/siimliimand",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "sf4\\api-user\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sf4\\api-user\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
